                        <h4 class="card-title">Update Navbar</h4>
            
            
                        <form id="homeForm" action="/navbarinfo" method="POST" style="display: none;">
                          <div class="form-group">
                            <label for="home">Home</label>
                            <input type="text" id="home" name="home" class="form-control" value="<%= navbar.home %>" required>
                          </div>
                          <button type="submit" class="btn btn-primary">Update Home</button>
                        </form>
            
           
                        <form id="aboutForm" action="/navbarinfo" method="POST" style="display: none;">
                          <div class="form-group">
                            <label for="about">About Us</label>
                            <input type="text" id="about" name="about" class="form-control" value="<%= navbar.about %>" required>
                          </div>
                          <button type="submit" class="btn btn-primary">Update About Us</button>
                        </form>
            
                        <form id="servicesForm" action="/navbarinfo" method="POST" style="display: none;">
                          <div class="form-group">
                            <label for="services">Services</label>
                            <input type="text" id="services" name="services" class="form-control" value="<%= navbar.services %>" required>
                          </div>
                          <button type="submit" class="btn btn-primary">Update Services</button>
                        </form>
       
                        <form id="faqsForm" action="/navbarinfo" method="POST" style="display: none;">
                          <div class="form-group">
                            <label for="faqs">FAQ</label>
                            <input type="text" id="faqs" name="faqs" class="form-control" value="<%= navbar.faqs %>" required>
                          </div>
                          <button type="submit" class="btn btn-primary">Update FAQ</button>
                        </form>
     
                        <form id="testimonialsForm" action="/navbarinfo" method="POST" style="display: none;">
                          <div class="form-group">
                            <label for="testimonials">Testimonials</label>
                            <input type="text" id="testimonials" name="testimonials" class="form-control" value="<%= navbar.testimonials %>" required>
                          </div>
                          <button type="submit" class="btn btn-primary">Update Testimonials</button>
                        </form>
            
                
                        <form id="contactForm" action="/navbarinfo" method="POST" style="display: none;">
                          <div class="form-group">
                            <label for="contact">Contact Us</label>
                            <input type="text" id="contact" name="contact" class="form-control" value="<%= navbar.contact %>" required>
                          </div>
                          <button type="submit" class="btn btn-primary">Update Contact Us</button>
                        </form>
            
              
                        <form id="setupForm" action="/navbarinfo" method="POST" style="display: none;">
                          <div class="form-group">
                            <label for="setup">SETUP</label>
                            <input type="text" id="setup" name="setup" class="form-control" value="<%= navbar.setup %>" required>
                          </div>
                          <button type="submit" class="btn btn-primary">Update SETUP</button>
                        </form>
            
                        <form id="pictureForm" action="/navbarinfo" method="POST" enctype="multipart/form-data" style="display: none;">
                          <div class="form-group">
                            <label for="picture">Navbar Logo</label>
                            <input type="file" id="picture" name="picture" class="form-control-file">
                          </div>
                          <button type="submit" class="btn btn-primary">Update Logo</button>
                        </form>
                      </div>
                    </div>
